citHeader("To cite fauxnaif in publications use:")

citEntry(
  entry    = "Manual",
  title    = "fauxnaif: Convert values to NA",
  author   = "Alexander Rossell Hayes",
  year     = 2019,
  note     = "R package version 0.3.0",
  url      = "https://github.com/rosellhayes/fauxnaif",
  textVersion = paste(
    "Alexander Rossell Hayes (2019).",
    "fauxnaif: Convert values to NA.",
    "R package version 0.3.0.",
    "https://github.com/rosellhayes/fauxnaif"
  )
)
